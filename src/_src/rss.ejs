<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <atom:link href="<%= data.settings.url %>/index.xml" rel="self" type="application/rss+xml" />

        <title><%= data.settings.title %></title>
        <link><%= data.settings.url %></link>
        <description><%= data.settings.description %></description>

        <% Object.keys(data.posts).forEach((postKey) => { %>
            <% const post = data.posts[postKey] %>
            <item>
                <guid><%= postKey %></guid>
                <title><%= post.meta.title %></title>
                <link><%= data.settings.url %>/posts/<%= postKey %>.html</link>
                <description>
                    <%= post.html %>
                </description>
            </item>
        <% }) %>
    </channel>
</rss>
